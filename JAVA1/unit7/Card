package unit7;

public class Card {
	private String suit;
	private int num;
	
	public Card(String suit, int num) {
		this.suit = suit;
		this.num = num;
	}
	
	public void print() {
		System.out.println(suit +" "+num);
	}
	
	public String getSuit() {
		return suit;
	}
	
	public int getNum() {
		return num;
	}
}
